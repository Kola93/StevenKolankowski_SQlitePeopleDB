<Window x:Class="WPF_UI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPF_UI"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="BTN_CSV_Browse" Content="Browse" HorizontalAlignment="Left" Margin="658,56,0,0" VerticalAlignment="Top" Width="76" Click="BTN_Browse_Click"/>
        <TextBox x:Name="TXT_Box_CSV_Directory" HorizontalAlignment="Left" Height="23" Margin="86,56,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="535"/>
        <Button x:Name="BTN_Import" Content="Import" HorizontalAlignment="Left" Margin="658,207,0,0" VerticalAlignment="Top" Width="76" Click="BTN_Import_Click" IsEnabled="False"/>
        <CheckBox x:Name="CB_Csv" Content="Upload new .csv file" HorizontalAlignment="Left" Margin="66,30,0,0" VerticalAlignment="Top" Checked="CB_Csv_Checked"/>
        <CheckBox x:Name="CB_Database" Content="Use existing database" HorizontalAlignment="Left" Margin="66,118,0,0" VerticalAlignment="Top" Checked="CB_Database_Checked"/>
        <TextBox x:Name="TXT_Box_Database_Directory" HorizontalAlignment="Left" Height="23" Margin="86,145,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="535"/>
        <Button x:Name="BTN_Database_Browse" Content="Browse" HorizontalAlignment="Left" Margin="658,148,0,0" VerticalAlignment="Top" Width="76" Click="BTN_Browse_Click" IsEnabled="False"/>
        <Grid HorizontalAlignment="Left" Height="229" Margin="47,190,0,0" VerticalAlignment="Top" Width="238">
            <TextBlock x:Name="TXT_Block_MostCommonEmailDomain_Name_1" HorizontalAlignment="Left" Margin="3,55,0,0" Text="x" TextWrapping="Wrap" VerticalAlignment="Top" Width="93"/>
            <TextBlock x:Name="TXT_Block_MostCommonEmailDomain_Name_2" HorizontalAlignment="Left" Margin="3,75,0,0" Text="x" TextWrapping="Wrap" VerticalAlignment="Top" Width="93"/>
            <TextBlock x:Name="TXT_Block_MostCommonEmailDomain_Name_3" HorizontalAlignment="Left" Margin="3,95,0,0" Text="x" TextWrapping="Wrap" VerticalAlignment="Top" Width="93"/>
            <TextBlock x:Name="TXT_Block_MostCommonEmailDomain_Value_1" HorizontalAlignment="Left" Margin="123,55,0,0" Text="x" TextWrapping="Wrap" VerticalAlignment="Top"/>
            <TextBlock x:Name="TXT_Block_MostCommonEmailDomain_Value_2" HorizontalAlignment="Left" Margin="123,75,0,0" Text="x" TextWrapping="Wrap" VerticalAlignment="Top"/>
            <TextBlock x:Name="TXT_Block_MostCommonEmailDomain_Value_3" HorizontalAlignment="Left" Margin="123,95,0,0" Text="x" TextWrapping="Wrap" VerticalAlignment="Top"/>
            <TextBlock HorizontalAlignment="Left" Margin="0,0,0,0" TextWrapping="Wrap" Text="Most common email addresses" VerticalAlignment="Top"/>
            <TextBlock HorizontalAlignment="Left" Margin="123,34,0,0" TextWrapping="Wrap" Text="Records" VerticalAlignment="Top" FontWeight="Bold"/>
            <TextBlock HorizontalAlignment="Left" Margin="0,34,0,0" TextWrapping="Wrap" Text="Email" VerticalAlignment="Top" FontWeight="Bold"/>
        </Grid>
       
       
        

    </Grid>
</Window>
